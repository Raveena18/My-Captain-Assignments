#include<stdio.h>
main()
{
	int n;
	printf("Enter marks:");
	scanf("%d",&n);
	if (n>=85)
	{
		printf("The grade acquired is: Grade A.\n");
	}
    else  if (n>=70)
	{
		printf("The grade acquired is: Grade B.\n");
	}
	else if (n>=55)
	{
		printf("The grade acquired is: Grade C.\n");
	}
	else if (n>=40)
	{
		printf("The grade acquired is: Grade D.\n");
	}
	else 
	{
		printf("the grade acquired is: Grade F.\n");
	}
	return 0;
	
}
